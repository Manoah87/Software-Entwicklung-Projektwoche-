<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="hfupilot.app.Views.StundenplanView">
    <ContentPage.Content>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Frame Grid.Row="0" BorderColor="GreenYellow"/>
            <Frame Grid.Row="1" BorderColor="Red"/>
            <Frame Grid.Row="2" BorderColor="Violet"/>
        </Grid>
    </ContentPage.Content>
</ContentPage>